        </Field>
        <Field Name="Count">
          <DataField>Count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="RepeatCount">
          <DataField>RepeatCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Severity">
          <DataField>Severity</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="TopAlertsByRemedyQueue">
      <Query>
        <DataSourceName>SCORE</DataSourceName>
        <CommandText>SELECT TOP 10 RANK() OVER (ORDER BY Count(*) DESC) as [RANK], AssignedTo as CustomField1, Count(*) as [Count]
FROM (
SELECT
	CustomField9 as AssignedTo
FROM scom.Alert
WHERE ResolutionState &lt; 254) t
GROUP BY AssignedTo
ORDER BY Count(*) DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="CustomField1">
