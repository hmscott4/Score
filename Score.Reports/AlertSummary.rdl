    <DataSet Name="TopAlertsByCount">
      <Query>
        <DataSourceName>SCORE</DataSourceName>
        <CommandText>SELECT TOP 5 RANK() OVER (ORDER BY Count(*) DESC) as [RANK], Name, Severity, Count(*) as [Count]
FROM scom.Alert
WHERE TimeRaised &gt; DateAdd(Hour, -24, GetUTCDate())
GROUP BY Name, Severity
ORDER BY Count(*) DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RANK">
          <DataField>RANK</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="Count">
          <DataField>Count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Severity">
          <DataField>Severity</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="TopAlertsByRepeatCount">
      <Query>
        <DataSourceName>SCORE</DataSourceName>
        <CommandText>SELECT TOP 5 RANK() OVER (ORDER BY Sum(RepeatCount) DESC) as [RANK], Name, Severity, Count(*) as [Count], Sum(RepeatCount) as [RepeatCount]
FROM scom.Alert
WHERE LastModified &gt; DateAdd(Hour, -24, GetUTCDate())
AND [Severity] &gt; 0
GROUP BY Name, Severity
ORDER BY Sum(RepeatCount) DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RANK">
