    <DataSet Name="AlertsByDay">
      <Query>
        <DataSourceName>SCORE</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UTCOffset">
            <Value>=Parameters!UTCOffset.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT a.Name, 
a.MonitoringObjectName, 
Category, 
[TimeRaised], 
cast(floor(cast(cast(a.[TimeRaised] as datetime) as float)) as [datetime]) [DateRaised], 
[TimeResolved], 
cast(floor(cast(cast(a.[TimeResolved]as datetime) as float)) as [datetime]) [DateResolved],
DATEADD(Minute, @UTCOffset, [TimeRaised]) AS [TimeRaisedLocal], 
cast(floor(cast(cast(DATEADD(Minute, @UTCOffset, [TimeRaised]) as datetime) as float)) as [datetime]) [DateRaisedLocal], 
DATEADD(Minute, @UTCOffset, [TimeResolved]) AS [TimeResolvedLocal], 
cast(floor(cast(DATEADD(Minute, @UTCOffset, cast([TimeResolved] as [datetime])) as float)) as [datetime]) [DateResolvedLocal],
a.Severity as [SeverityID],
CASE a.Severity
 WHEN 0 THEN 'Info'
 WHEN 1 THEN 'Warning'
 WHEN 2 THEN 'Critical'
END as [Severity], 
a.Priority as [PriorityID],
CASE a.Priority
  WHEN 0 THEN 'Low'
  WHEN 1 THEN 'Medium'
  WHEN 2 THEN 'High'
END as [Priority], 
a.ResolutionState,
r.Name as ResolutionStateName, 
(DATEDIFF(Minute, [TimeRaised], GETUTCDATE()) /60 ) as AgeHours,
b.SortOrder,
b.Label,
b.LowValue,
b.HighValue
FROM 
	scom.Alert a inner join scom.AlertResolutionState r
		on a.ResolutionState = r.ResolutionState
	INNER JOIN scom.AlertAgingBuckets b on
		(DATEDIFF(Minute, [TimeRaised], GETUTCDATE()) / 60) BETWEEN b.LowValue and b.HighValue

ORDER BY CAST([TimeRaised] as [date]) ASC</CommandText>
      </Query>
