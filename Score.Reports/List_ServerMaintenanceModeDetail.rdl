    <DataSet Name="MaintenanceModeReason">
      <Query>
        <DataSourceName>Catalog</DataSourceName>
        <CommandText>SELECT 
    ReasonCode, 
    Reason
FROM 
    scom.MaintenanceReasonCode
WHERE 
    Active = 1
ORDER BY ReasonCode</CommandText>
      </Query>
      <Fields>
        <Field Name="ReasonCode">
          <DataField>ReasonCode</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="Reason">
          <DataField>Reason</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
