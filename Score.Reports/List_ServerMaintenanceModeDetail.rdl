  <DataSets>
    <DataSet Name="MaintenanceModeDetail">
      <Query>
        <DataSourceName>OperationsManagerDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@WindowStart">
            <Value>=Parameters!WindowStart.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@WindowEnd">
            <Value>=Parameters!WindowEnd.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @StandardTimeZone nvarchar(255)

SET @StandardTimeZone = N'Coordinated Universal Time'

SELECT 
	me.FullName, 
	me.DisplayName, 
	mm.StartDateTime, 
	mmh.ScheduledEndDateTime, 
	mm.EndDateTime, 
	DateDiff(Minute,mm.StartDateTime, mm.EndDateTime) [Duration],
	mm.PlannedMaintenanceInd,
	CAST(CONVERT(varchar(14), mm.StartDateTime, 120)  + '00' AS DateTime) AS WindowStart,
	CAST(CONVERT(varchar(14), mm.EndDateTime, 120)  + '00' AS DateTime) AS WindowEnd,
                DateDiff(Minute,CAST(CONVERT(varchar(14), mm.StartDateTime, 120)  + '00' AS DateTime), CAST(CONVERT(varchar(14), mm.EndDateTime, 120)  + '00' AS DateTime)) AS [WindowDuration],
	--Cast(mm.StartDateTime as datetimeoffset) AT TIME ZONE @StandardTimeZone AS Local_StartDateTime,
	--Cast(mm.EndDateTime as datetimeoffset) AT TIME ZONE @StandardTimeZone AS Local_EndDateTime,
	--Cast(mmh.ScheduledEndDateTime as datetimeoffset) AT TIME ZONE @StandardTimeZone AS Local_ScheduledDateTime,
	mmh.ReasonCode,
	mmh.UserId,
	mmh.Comment
FROM 
	MaintenanceMode mm inner join MaintenanceModeHistory mmh
		on mm.MaintenanceModeRowId = mmh.MaintenanceModeRowId
	inner join ManagedEntity me on
		mm.ManagedEntityRowId = me.ManagedEntityRowId
WHERE
	CAST(CONVERT(varchar(14), mm.StartDateTime, 120)  + '00' AS DateTime) = @WindowStart 
                AND CAST(CONVERT(varchar(14), mm.EndDateTime, 120)  + '00' AS DateTime) = @WindowEnd
	AND FullName Like N'Microsoft.Windows.Computer:%'
ORDER BY 
	StartDateTime</CommandText>
      </Query>
      <Fields>
        <Field Name="FullName">
          <DataField>FullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DisplayName">
          <DataField>DisplayName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StartDateTime">
          <DataField>StartDateTime</DataField>
