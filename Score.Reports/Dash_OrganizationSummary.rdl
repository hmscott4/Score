  <DataSets>
    <DataSet Name="ObjectStateSummary">
      <Query>
        <DataSourceName>SCORE</DataSourceName>
        <CommandText>SELECT GenericName, HealthState, IsAvailable, InMaintenanceMode
FROM (
SELECT
	oc.genericname, 
	case 
		when InMaintenanceMode = 1 then 'MaintenanceMode'
		when IsAvailable=0 then 'NotAvailable' 
		else HealthState
		
	end as [HealthState],

	IsAvailable, 
	InMaintenanceMode 
FROM 
	scom.[ObjectHealthState] o inner join scom.[ObjectClass] oc 
		on o.ObjectClass = oc.Name
WHERE
	o.Active = 1
	AND oc.Active = 1) tmp
WHERE
	tmp.HealthState IN (N'Success',N'Warning',N'Error',N'MaintenanceMode',N'NotAvailable')</CommandText>
      </Query>
      <Fields>
        <Field Name="genericname">
          <DataField>GenericName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="HealthState">
          <DataField>HealthState</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="IsAvailable">
          <DataField>IsAvailable</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
        <Field Name="InMaintenanceMode">
          <DataField>InMaintenanceMode</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
