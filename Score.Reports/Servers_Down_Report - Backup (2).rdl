  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="OperationsManager">
      <DataSourceReference>OperationsManager</DataSourceReference>
      <rd:SecurityType>Integrated</rd:SecurityType>
      <rd:DataSourceID>47605b18-e8ca-4c2c-9660-49084096c2b2</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Servers_Down">
      <Query>
        <DataSourceName>OperationsManager</DataSourceName>
        <CommandText>SELECT bme.DisplayName, s.LastModified as LastModifiedUTC, dateadd(hh,-5,s.LastModified) as 'LastModifiedCST (GMT-5)'
FROM state AS s, BaseManagedEntity as bme 
WHERE s.basemanagedentityid = bme.basemanagedentityid AND s.monitorid 
IN (SELECT MonitorId FROM Monitor WHERE MonitorName = 'Microsoft.SystemCenter.HealthService.ComputerDown') 
AND s.Healthstate = '3' AND bme.IsDeleted = '0' 
ORDER BY s.Lastmodified DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="DisplayName">
          <DataField>DisplayName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LastModifiedUTC">
          <DataField>LastModifiedUTC</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="LastModifiedCST__GMT_5_">
          <DataField>LastModifiedCST (GMT-5)</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
