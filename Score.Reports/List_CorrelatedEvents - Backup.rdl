  <DataSets>
    <DataSet Name="CorrelatedEvents">
      <Query>
        <DataSourceName>OperationsManagerDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ServerName">
            <Value>=Parameters!ServerName.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT TOP 1000 elc.ComputerName, evt.DateTime, evt.EventDisplayNumber, replace(replace(evtd.RenderedDescription,char(10), ''), char(13), '') as EventDescription, null as AlertLastModified, N'Event' as [Source], null as LastBoot
FROM 
	event.vEvent evt inner join event.vEventDetail evtd on evt.EventOriginId = evtd.EventOriginId
	inner join EventLoggingComputer elc on evt.LoggingComputerRowId = elc.EventLoggingComputerRowId
WHERE
	evt.EventDisplayNumber in (6008, 6005, 6006)
	AND elc.ComputerName = @ServerName
	AND evt.DateTime &gt; GetUTCDate()-7

UNION ALL

SELECT me.DisplayName, a.RaisedDateTime, null, a.AlertName, a.DWLastModifiedDateTime, N'Alert', null
FROM [Alert].[vAlert] a
inner join ManagedEntity me on a.ManagedEntityRowId = me.ManagedEntityRowId
WHERE a.AlertName = 'Health Service Heartbeat Failure'
AND me.DisplayName = @ServerName
AND a.RaisedDateTime &gt; GetUTCDate()-7



UNION ALL

SELECT me.DisplayName,  vpr.DateTime, null, N'System Up Time (' + cast(vpr.SampleValue as nvarchar(255)) + ' seconds)' , Null, N'System Up Time', DateAdd(SECOND,-vpr.sampleValue,vpr.datetime)
FROM 
	perf.vPerfRaw vpr inner join PerformanceRuleInstance pri on vpr.PerformanceRuleInstanceRowId = pri.PerformanceRuleInstanceRowId
	inner join PerformanceRule pr on pri.RuleRowId = pr.RuleRowId
	inner join ManagedEntity me on vpr.ManagedEntityRowId = me.ManagedEntityRowId
WHERE 
	me.DisplayName = @ServerName
	and pr.CounterName = N'System Up Time'
	AND vpr.DateTime &gt; GetUTCDate()-7

ORDER BY DateTime DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="ComputerName">
          <DataField>ComputerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
