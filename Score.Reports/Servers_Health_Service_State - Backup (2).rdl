  <DataSets>
    <DataSet Name="Server_Health_State">
      <Query>
        <DataSourceName>OperationsManager</DataSourceName>
        <CommandText>SELECT bme.DisplayName, s.LastModified as LastModifiedUTC, dateadd(hh,-5,s.LastModified) as 'LastModifiedCST (GMT-5)', s.Healthstate as State
FROM state AS s, BaseManagedEntity as bme 
WHERE s.basemanagedentityid = bme.basemanagedentityid AND s.monitorid 
IN (SELECT MonitorId FROM Monitor WHERE MonitorName = 'Microsoft.SystemCenter.HealthService.ComputerDown') 
AND bme.IsDeleted = '0' 
ORDER BY s.Lastmodified DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="DisplayName">
          <DataField>DisplayName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LastModifiedUTC">
          <DataField>LastModifiedUTC</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="LastModifiedCST__GMT_5_">
          <DataField>LastModifiedCST (GMT-5)</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="State">
          <DataField>State</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
