    <DataSet Name="AlertDetail">
      <Query>
        <DataSourceName>Catalog</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Severity">
            <Value>=Parameters!Severity.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UTCOffset">
            <Value>=Parameters!UTCOffset.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ResolutionState">
            <Value>=Parameters!ResolutionState.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@QueueName">
            <Value>=Parameters!QueueName.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
                a.AlertID,
	a.Name, 
	IsNull(a.MonitoringObjectName, '') as MonitoringObjectName, 
	IsNull(a.MonitoringObjectPath, '') as MonitoringObjectPath,
	a.MonitoringObjectDisplayName,
	Category, 
	[TimeRaised], 
	cast(floor(cast(cast(a.[TimeRaised] as datetime) as float)) as [datetime]) [DateRaised], 
	[TimeResolved], 
	cast(floor(cast(cast(a.[TimeResolved]as datetime) as float)) as [datetime]) [DateResolved],
	DATEADD(Minute, @UTCOffset, [TimeRaised]) AS [TimeRaisedLocal], 
	cast(floor(cast(cast(DATEADD(Minute, @UTCOffset, [TimeRaised]) as datetime) as float)) as [datetime]) [DateRaisedLocal], 
	DATEADD(Minute, @UTCOffset, [TimeResolved]) AS [TimeResolvedLocal], 
	cast(floor(cast(DATEADD(Minute, @UTCOffset, cast([TimeResolved] as [datetime])) as float)) as [datetime]) [DateResolvedLocal],
	CASE a.Severity
		WHEN 0 THEN 'Info'
		WHEN 1 THEN 'Warning'
		WHEN 2 THEN 'Critical'
	END as [Severity], 
	CASE a.Priority
		WHEN 0 THEN 'Low'
		WHEN 1 THEN 'Medium'
		WHEN 2 THEN 'High'
	END as [Priority], 
	r.Name as ResolutionStateName, 
	DATEDIFF(Hour, [TimeRaised], GETUTCDATE()) AgeHours,
	a.RepeatCount
FROM 
	scom.Alert a inner join scom.AlertResolutionState r
		on a.ResolutionState = r.ResolutionState
WHERE
	a.Severity IN (@Severity) AND
	a.ResolutionState in (@ResolutionState) AND
	DATEADD(Minute, @UTCOffset, [TimeRaised]) BETWEEN @StartDate AND @EndDate
	AND (a.CustomField9 IN (@QueueName) )
	AND a.Active = 1</CommandText>
      </Query>
