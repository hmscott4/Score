    <DataSet Name="QueueNames">
      <Query>
        <DataSourceName>Catalog</DataSourceName>
        <CommandText>SELECT DISTINCT
    CustomField9 AS QueueName
FROM
    scom.Alert
WHERE
   CustomField9 != '' 
    AND CustomField9 IS NOT NULL
ORDER BY 1</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
