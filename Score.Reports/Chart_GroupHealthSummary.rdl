  <DataSets>
    <DataSet Name="GroupStateSummary">
      <Query>
        <DataSourceName>SCORE</DataSourceName>
        <CommandText>
SELECT
	g.DisplayName, 
	case 
		when InMaintenanceMode = 1 then 'MaintenanceMode'
		when IsAvailable=0 then 'NotAvailable' 
		else HealthState
		
	end as [HealthState],
	IsAvailable, 
	InMaintenanceMode 
FROM 
	scom.[GroupHealthState] g
WHERE
	g.Active = 1
                   AND g.Display = 1</CommandText>
      </Query>
      <Fields>
        <Field Name="DisplayName">
          <DataField>DisplayName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="HealthState">
          <DataField>HealthState</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="IsAvailable">
          <DataField>IsAvailable</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
        <Field Name="InMaintenanceMode">
          <DataField>InMaintenanceMode</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
