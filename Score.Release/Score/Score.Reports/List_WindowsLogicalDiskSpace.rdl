    <DataSet Name="LogicalDisk">
      <Query>
        <DataSourceName>OperationsManagerDW</DataSourceName>
        <CommandText>WITH CTE_LogicalDiskSpace ([Path], FullName, ObjectName, CounterName, InstanceName, SampleValue, [DateTime])
AS (
SELECT Path, FullName, ObjectName, CounterName, InstanceName, SampleValue, DateTime 
FROM 
Perf.vPerfRaw pvpr INNER JOIN vManagedEntity vme 
	ON pvpr.ManagedEntityRowId = vme.ManagedEntityRowId 
INNER JOIN vPerformanceRuleInstance vpri 
	ON pvpr.PerformanceRuleInstanceRowId = vpri.PerformanceRuleInstanceRowId 
INNER JOIN vPerformanceRule vpr 
	ON vpr.RuleRowId = vpri.RuleRowId 
WHERE 
	vpr.ObjectName IN ( N'LogicalDisk',N'Cluster Disk',N'Cluster Shared Volume')
	AND vpr.CounterName IN (N'% Free Space', N'Free Space / Percent')
	AND pvpr.DateTime &gt; DateAdd(Minute, -180, GetUTCDate())
)

SELECT td.Path, FullName, ObjectName, CounterName, td.InstanceName, SampleValue, td.DateTime
FROM CTE_LogicalDiskSpace td INNER JOIN (

	SELECT path, instancename, max(DateTime) as [DateTime] 
	FROM CTE_LogicalDiskSpace 
	GROUP BY path, instancename) td_last 
	
	on td.Path = td_last.Path and td.InstanceName = td_last.InstanceName and td.DateTime = td_last.DateTime</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Path">
          <DataField>Path</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
