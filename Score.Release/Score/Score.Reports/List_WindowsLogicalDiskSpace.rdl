    <DataSet Name="WindowsLogicalDiskHealth">
      <Query>
        <DataSourceName>SCORE</DataSourceName>
        <CommandText>SELECT
    ohs.ID,
    ohs.Name,
    ohs.DisplayName,
    ohs.FullName,
    ohs.Path,
    ohs.ObjectClass,
    ohs.HealthState,
    ohs.IsAvailable,
    ohs.InMaintenanceMode,
    ohs.Active,
    ohs.Availability
FROM
    scom.ObjectHealthState ohs INNER JOIN scom.ObjectClass oc ON
        ohs.ObjectClass = oc.Name
WHERE
    oc.GenericName='Windows Logical Disk'
    AND ohs.Active = 1
    AND oc.Active = 1</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
