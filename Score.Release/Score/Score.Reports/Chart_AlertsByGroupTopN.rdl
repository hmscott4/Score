  <DataSets>
    <DataSet Name="TopNAlerts">
      <Query>
        <DataSourceName>OperationsManagerDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ManagedGroupName">
            <Value>=Parameters!ManagedGroupName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=DateAdd(DateInterval.Day, 1, Parameters!EndDate.Value)</Value>
          </QueryParameter>
          <QueryParameter Name="@TopN">
            <Value>=Parameters!TopN.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@UTCOffset">
            <Value>=Parameters!UTCOffset.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@AlertSeverity">
            <Value>=Parameters!AlertSeverity.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- *** Get the group members ***
DECLARE @ComputerGroupMembers
TABLE (ComputerID INT, DisplayName NVARCHAR(250))

INSERT INTO @ComputerGroupMembers
SELECT DISTINCT rel.TargetManagedEntityRowId, vMENT_1.DisplayName
FROM ManagedEntity AS vMENT
INNER JOIN  Relationship AS rel ON vMENT.ManagedEntityRowId = rel.SourceManagedEntityRowId
INNER JOIN  ManagedEntity AS vMENT_1 ON rel.TargetManagedEntityRowId = vMENT_1.ManagedEntityRowId
WHERE vMENT.DisplayName = @ManagedGroupName 

-- *** Get the Alerts ***
DECLARE @AlertList 
TABLE (AlertDisplayName NVARCHAR(max), AlertDescription NVARCHAR(max))

-- Build List of Alerts filtered by date
INSERT INTO @AlertList
SELECT  vAlrt.AlertName, vAlrt.AlertDescription
FROM Alert.vAlert vAlrt WITH (NOLOCK)
JOIN VManagedEntity AS vMENT ON vAlrt.ManagedEntityRowId = vMENT.ManagedEntityRowId
INNER JOIN @ComputerGroupMembers ON ComputerID = vMENT.TopLevelHostManagedEntityRowId
WHERE vAlrt.RaisedDateTime BETWEEN DATEADD(Minute, @UTCOffset, @StartDate) AND DATEADD(Minute, @UTCOffset, @EndDate)
AND vAlrt.Severity IN (@AlertSeverity)
GROUP BY AlertName, AlertDescription
Order by AlertName

-- *** Add the alerts together ***
DECLARE @AlertCountList 
TABLE ( AlertDisplayName NVARCHAR(max), AlertCount INTEGER)

-- Build a list of new alerts
INSERT INTO @AlertCountList
SELECT AlertDisplayName, SUM(1) AS AlertCount
FROM @AlertList
GROUP BY AlertDisplayName
ORDER BY AlertCount DESC

-- Get the Top 25 Alerts
SELECT TOP (@TopN) AlertDisplayName, AlertCount
FROM @AlertCountList
ORDER BY AlertCount DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="AlertDisplayName">
          <DataField>AlertDisplayName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="AlertCount">
          <DataField>AlertCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="AlertsByDate">
      <Query>
        <DataSourceName>OperationsManagerDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ManagedGroupName">
            <Value>=Parameters!ManagedGroupName.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=DateAdd(DateInterval.Day, 1, Parameters!EndDate.Value)</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@UTCOffset">
            <Value>=Parameters!UTCOffset.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@AlertSeverity">
            <Value>=Parameters!AlertSeverity.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- *** Get the group members ***
DECLARE @ComputerGroupMembers
TABLE (ComputerID INT, DisplayName NVARCHAR(250))

INSERT INTO @ComputerGroupMembers
SELECT DISTINCT rel.TargetManagedEntityRowId, vMENT_1.DisplayName
FROM ManagedEntity AS vMENT
INNER JOIN  Relationship AS rel ON vMENT.ManagedEntityRowId = rel.SourceManagedEntityRowId
INNER JOIN  ManagedEntity AS vMENT_1 ON rel.TargetManagedEntityRowId = vMENT_1.ManagedEntityRowId
WHERE vMENT.DisplayName = @ManagedGroupName 

-- *** Get the Alerts ***
DECLARE @AlertList 
TABLE (AlertDisplayName NVARCHAR(max), AlertDescription NVARCHAR(max), RaisedDateTime DATETIME, Severity INT, RepeatCount BIGINT)

-- Build List of Alerts filtered by date
INSERT INTO @AlertList
SELECT  vAlrt.AlertName, vAlrt.AlertDescription, vAlrt.RaisedDateTime, vAlrt.Severity, vAlrt.Severity
FROM Alert.vAlert vAlrt WITH (NOLOCK)
JOIN VManagedEntity AS vMENT ON vAlrt.ManagedEntityRowId = vMENT.ManagedEntityRowId
INNER JOIN @ComputerGroupMembers ON ComputerID = vMENT.TopLevelHostManagedEntityRowId
WHERE vAlrt.RaisedDateTime BETWEEN DATEADD(Minute, @UTCOffset, @StartDate) AND DATEADD(Minute, @UTCOffset, @EndDate) AND vAlrt.Severity IN (@AlertSeverity)
ORDER BY AlertName

SELECT Cast(Convert(Varchar(10), RaisedDateTime, 120) as DATETIME) as RaisedDate, Severity, Count(*) as AlertCount, Sum(RepeatCount) as RepeatCount
FROM @AlertList
GROUP BY Cast(Convert(Varchar(10), RaisedDateTime, 120) as DATETIME), Severity</CommandText>
      </Query>
