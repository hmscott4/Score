  <DataSets>
    <DataSet Name="DatabaseSpaceAllocated">
      <Query>
        <DataSourceName>OperationsManagerDW</DataSourceName>
        <CommandText>DECLARE @StartDate datetime
DECLARE @EndDate datetime
DECLARE @ObjectName nvarchar(255)
DECLARE @CounterName nvarchar(255)

SET @StartDate = DateAdd(Hour, -4, GetUTCDate())
SET @EndDate = GETUTCDATE()
SET @ObjectName = N'SQL Database'

SELECT 
	case
		when CHARINDEX(';',vme.Path) &gt; 0 THEN REPLACE(vme.Path, RIGHT(vme.Path, (LEN(vme.Path) - CHARINDEX(';',vme.Path))), '')
		else REPLACE(vme.Path, RIGHT(vmeTop.DisplayName, (LEN(vmeTop.DisplayName) - CHARINDEX('\', vmeTop.DisplayName))), '')
	end as ServerName,
	case
		when CHARINDEX(';',vme.Path) &gt; 0 THEN RIGHT(vme.Path, (LEN(vme.Path) - CHARINDEX(';',vme.Path)))
		else RIGHT(vmeTop.DisplayName, (LEN(vmeTop.DisplayName) - CHARINDEX('\', vmeTop.DisplayName)))
	end as InstanceName,
	--vmetop.Name,
	--vmeTop.DisplayName,
	--vmeTop.FullName,
	vme.Path, 
	vme.DisplayName , 
	vme.FullName, 
	vpr.ObjectName, 
	vpr.CounterName, 
	AVG(SampleValue) [SpaceAllocated]
	-- StandardDeviation,
	-- [DateTime] 
FROM 
	Perf.vPerfRaw pvpr 
		INNER JOIN vManagedEntity vme on pvpr.ManagedEntityRowId = vme.ManagedEntityRowId 
		INNER JOIN vPerformanceRuleInstance vpri on pvpr.PerformanceRuleInstanceRowId = vpri.PerformanceRuleInstanceRowId 
		INNER JOIN vPerformanceRule vpr on vpr.RuleRowId = vpri.RuleRowId 
		INNER JOIN vManagedEntity vmeTop ON vme.TopLevelHostManagedEntityRowId = vmeTop.ManagedEntityRowId
WHERE 
	vpr.ObjectName = @ObjectName
	AND vpr.CounterName IN (N'DB Allocated Size (MB)',
		N'DB Allocated Space (MB)')
	AND pvpr.[datetime] BETWEEN @StartDate AND @EndDate
GROUP BY
	case
		when CHARINDEX(';',vme.Path) &gt; 0 THEN REPLACE(vme.Path, RIGHT(vme.Path, (LEN(vme.Path) - CHARINDEX(';',vme.Path))), '')
		else REPLACE(vme.Path, RIGHT(vmeTop.DisplayName, (LEN(vmeTop.DisplayName) - CHARINDEX('\', vmeTop.DisplayName))), '')
	end,
	case
		when CHARINDEX(';',vme.Path) &gt; 0 THEN RIGHT(vme.Path, (LEN(vme.Path) - CHARINDEX(';',vme.Path)))
		else RIGHT(vmeTop.DisplayName, (LEN(vmeTop.DisplayName) - CHARINDEX('\', vmeTop.DisplayName)))
	end,
	--vmetop.Name,
	--vmeTop.DisplayName,
	--vmeTop.FullName,
	vme.Path, 
	vme.DisplayName , 
	vme.FullName, 
	vpr.ObjectName, 
	vpr.CounterName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="IsSystemDB">
          <Value>=Iif(Fields!DisplayName.Value="master", 1, iif(Fields!DisplayName.Value="model", 1, iif(Fields!DisplayName.Value="msdb", 1, iif(Fields!DisplayName.Value="tempdb", 1, 0))))</Value>
        </Field>
        <Field Name="ServerName">
